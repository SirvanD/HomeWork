<h1>Say hi to the planets!</h1>

<a href="/add_planet">Add a planet!</a>

<section class="planets">
    <% planets.each do |planet|%>
        <h3>
            <a href="/planets/<%= planet['id']%>"><%= planet['name'] %></a>
        </h3>
        <img src="<%= planet['image_url']%>" alt="image of <%= planet['name'] %>">
    <%end%>    
</section>